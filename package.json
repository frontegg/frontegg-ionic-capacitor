{
  "name": "@frontegg/ionic-capacitor",
  "version": "2.0.7",
  "description": "Frontegg Ionic Capacitor SDK",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "FronteggIonicCapacitor.podspec"
  ],
  "author": "Frontegg LTD",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontegg/frontegg-ionic-capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/frontegg/frontegg-ionic-capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api FronteggNativePlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "sync": "npm run verify && npm pack && node scripts/postpack.js && cd example && ionic capacitor sync",
    "start:android": "cd example && ionic capacitor run android --livereload --external",
    "start:ios": "cd example && ionic capacitor run ios --livereload --external"
  },
  "dependencies": {
    "@frontegg/rest-api": "^3.1.56"
  },
  "devDependencies": {
    "@angular/core": ">=14.0.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/docgen": "^0.0.18",
    "@capacitor/ios": "^5.0.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.11.0",
    "prettier": "~2.3.0",
    "prettier-plugin-java": "~1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "swiftlint": "^1.0.1",
    "typescript": "~4.1.5",
    "@angular-eslint/builder": "^16.0.0",
    "@angular-eslint/eslint-plugin": "^16.0.0",
    "@angular-eslint/eslint-plugin-template": "^16.0.0",
    "@angular-eslint/schematics": "^16.0.0",
    "@angular-eslint/template-parser": "^16.0.0"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
